{%- set field = fields[0] %}
{%- set type_expr = field.annotated if field.annotated else field.type_hint %}
{%- if type_alias_format == 'type_stmt' -%}
type {{ class_name }} = {{ type_expr }}
{%- elif type_alias_format == 'type_alias_type' -%}
{{ class_name }} = TypeAliasType('{{ class_name }}', {{ type_expr }})
{%- elif type_alias_format == 'annotation' -%}
{{ class_name }}: TypeAlias = {{ type_expr }}
{%- else -%}
{{ class_name }} = {{ type_expr }}
{%- endif -%}
